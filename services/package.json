{
  "name": "services",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "start": "node -r ./config.js ./dist",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@prisma/client": "6.17.1",
    "bcrypt": "6.0.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dayjs": "1.11.18",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "inserturlparams": "2.0.5",
    "jet-logger": "2.0.1",
    "jet-paths": "1.1.0",
    "jet-validators": "1.4.3",
    "jsonfile": "6.2.0",
    "jsonwebtoken": "9.0.2",
    "module-alias": "2.2.3",
    "morgan": "1.10.1"
  },
  "devDependencies": {
    "@swc/core": "1.13.5",
    "@types/bcrypt": "6.0.0",
    "@types/cookie-parser": "1.4.9",
    "@types/cors": "2.8.19",
    "@types/find": "0.2.4",
    "@types/fs-extra": "11.0.4",
    "@types/jsonfile": "6.1.4",
    "@types/jsonwebtoken": "9.0.10",
    "@types/module-alias": "2.0.4",
    "@types/morgan": "1.9.10",
    "@types/node": "24.7.2",
    "@types/supertest": "6.0.3",
    "find": "0.3.0",
    "fs-extra": "11.3.2",
    "jiti": "2.6.1",
    "nodemon": "3.1.10",
    "prisma": "6.17.1",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}